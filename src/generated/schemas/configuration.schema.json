    "tracingEnabled": {
      "type": "boolean"
    },
    "loggingEnabled": {
      "type": "boolean"
    },
    "metricsEnabled": {
      "type": "boolean"
    },
    "matchPatterns": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "traceCollectorUrl": {
      "type": "string"
    },
    "logCollectorUrl": {
      "type": "string"
    },
    "metricCollectorUrl": {
      "type": "string"
    },
    "attributes": {
      "type": "object"
    },
    "headers": {
      "type": "object"
    },
